<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Webkul_MpVendorAttributeManager::css/datepicker.css"/>
    </head>
    <body>
        <referenceBlock name="footer_links">
            <block class="Webkul\MpVendorAttributeManager\Block\Account\RegisterLink" name="vendor-register">
                <arguments>
                    <argument name="label" xsi:type="string" translate="true">Vendor Registration</argument>
                </arguments>
            </block>
        </referenceBlock>
        <referenceBlock name="content">
          <block class="Magento\Framework\View\Element\Template" name="call_onall_pages" after="-"  template="Webkul_MpVendorAttributeManager::allpages.phtml" />
        </referenceBlock>
    </body>
</page>

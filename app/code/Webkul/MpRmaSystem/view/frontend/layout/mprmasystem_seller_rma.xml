<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <css src="Webkul_MpRmaSystem::css/styles.css"/>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">View RMA</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Webkul\MpRmaSystem\Block\Seller\Rma" name="mprmasystem_seller_rma" template="Webkul_MpRmaSystem::seller/rma.phtml" cacheable="false" />
        </referenceContainer>
    </body>
</page>
